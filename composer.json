{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-imagick": "*",
        "ext-json": "*",
        "api-platform/api-pack": "^1.2",
        "aws/aws-sdk-php": "^3.61",
        "evence/soft-deleteable-extension-bundle": "^1.3",
        "friendsofsymfony/elastica-bundle": "^5.0",
        "gesdinet/jwt-refresh-token-bundle": "^0.4.0",
        "google/apiclient": "^2.2",
        "jsor/doctrine-postgis": "^1.7",
        "knplabs/knp-snappy-bundle": "^1.5",
        "laupifrpar/pusher-bundle": "^3.0",
        "lcobucci/jwt": "^3.2",
        "league/csv": "^9.4",
        "lexik/jwt-authentication-bundle": "^2.6",
        "oro/doctrine-extensions": "^1.2",
        "sensio/framework-extra-bundle": "^5.5",
        "sentry/sentry-symfony": "^3.2",
        "sly/notification-pusher": "^2.3",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/amqp-pack": "^1.0",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "4.3.*",
        "symfony/console": "4.3.*",
        "symfony/filesystem": "^4.3",
        "symfony/finder": "^4.3",
        "symfony/flex": "^1.4",
        "symfony/form": "4.3.*",
        "symfony/framework-bundle": "^4.3",
        "symfony/messenger": "^4.3",
        "symfony/monolog-bundle": "^3.4",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "4.3.*",
        "symfony/swiftmailer-bundle": "^3.3",
        "symfony/templating": "^4.3",
        "symfony/translation": "4.3.*",
        "symfony/twig-pack": "^1.0",
        "symfony/validator": "4.3.*",
        "symfony/yaml": "^4.3",
        "twig/extensions": "^1.5"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "sensiolabs/security-checker": "^4.1",
        "symfony/debug-pack": "^1.0",
        "symfony/dotenv": "^4.1",
        "symfony/maker-bundle": "^1.11",
        "symfony/var-dumper": "^4.1",
        "symfony/web-server-bundle": "^4.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    }
}
